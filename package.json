{
  "name": "beaver",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub App built with Probot that logs actions",
  "author": "Enyil Padilla <enyil@github.com>",
  "license": "ISC",
  "homepage": "https://github.com/octodemo/beaver#readme",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:es": "eslint .",
    "lint:js": "standard",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:engines": "check-engine",
    "lint:peer": "npm ls >/dev/null",
    "test:coverage": "jest --coverage",
    "test:unit": "jest 'test/unit/'",
    "test:me": "jest ",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:integration": "jest 'test/integration/'",
    "test:integration:debug": "LOG_LEVEL=debug DEBUG=nock run-s test:integration"
  },
  "dependencies": {
    "@azure/event-hubs": "^5.11.0",
    "@elastic/ecs-pino-format": "^1.3.0",
    "@probot/adapter-aws-lambda-serverless": "^3.0.3",
    "axios": "^1.4.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "probot": "12.3.1",
    "proxy-agent": "^6.2.1"
  },
  "devDependencies": {
    "@travi/any": "^2.1.5",
    "eslint": "^8.42.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jest": "^29.5.0",
    "nock": "^13.3.1",
    "nodemon": "^2.0.22",
    "smee-client": "^1.2.2"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
